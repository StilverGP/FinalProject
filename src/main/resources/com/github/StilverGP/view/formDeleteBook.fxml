<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.github.StilverGP.view.DeleteBookController">
   <children>
      <AnchorPane prefHeight="231.0" prefWidth="383.0">
         <children>
            <TextField fx:id="codBook" layoutX="117.0" layoutY="119.0" promptText="Código de la reserva" />
            <Label layoutX="82.0" layoutY="96.0" text="Inserte el numero de la reserva a cancelar" />
            <Button layoutX="166.0" layoutY="163.0" mnemonicParsing="false" onAction="#deleteBook" text="Borrar" />
            <PasswordField fx:id="password" layoutX="116.0" layoutY="67.0" promptText="Contraseña" />
            <Label layoutX="118.0" layoutY="44.0" text="Inserte su contraseña actual" />
         </children>
      </AnchorPane>
   </children>
</VBox>

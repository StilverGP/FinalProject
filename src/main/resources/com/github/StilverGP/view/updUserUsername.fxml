<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.github.StilverGP.controller.UserUsernameController">
    <children>
        <AnchorPane prefHeight="231.0" prefWidth="383.0">
            <children>
                <TextField fx:id="username" layoutX="118.0" layoutY="120.0" promptText="Username" />
                <Label layoutX="112.0" layoutY="98.0" text="Introduzca el nuevo username" />
                <Button layoutX="158.0" layoutY="157.0" mnemonicParsing="false" onAction="#updateUser" text="Actualizar" />
            <PasswordField fx:id="password" layoutX="118.0" layoutY="69.0" promptText="Contraseña" />
            <Label layoutX="108.0" layoutY="49.0" text="Introduzca su contraseña actual" />
            </children>
        </AnchorPane>
    </children>
</VBox>
